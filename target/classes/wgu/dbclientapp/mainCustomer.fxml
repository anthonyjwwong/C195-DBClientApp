<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="549.0" prefWidth="849.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wgu.dbclientapp.controller.mainCustomerController">
   <children>
      <TableView fx:id="tableCustomer" layoutX="57.0" layoutY="85.0" prefHeight="297.0" prefWidth="735.0">
        <columns>
          <TableColumn fx:id="tableCustomer_id" prefWidth="61.0" text="ID" />
            <TableColumn fx:id="tableCustomer_name" prefWidth="130.0" text="Name" />
            <TableColumn fx:id="tableCustomer_address" prefWidth="254.0" text="Address" />
            <TableColumn fx:id="tableCustomer_postal" prefWidth="165.0" text="Postal Code" />
          <TableColumn fx:id="tableCustomer_phone" prefWidth="124.0" text="Phone" />
        </columns>
      </TableView>
      <Label layoutX="57.0" layoutY="52.0" text="Customer Information">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="addCustomerBtn" layoutX="57.0" layoutY="395.0" mnemonicParsing="false" onAction="#addCustomer" prefHeight="25.0" prefWidth="66.0" text="Add" />
      <Button fx:id="updateBtn" layoutX="141.0" layoutY="395.0" mnemonicParsing="false" onAction="#updateCustomer" prefHeight="25.0" prefWidth="66.0" text="Update" />
      <Button fx:id="deleteButton" layoutX="726.0" layoutY="395.0" mnemonicParsing="false" onAction="#deleteCustomer" prefHeight="25.0" prefWidth="66.0" text="Delete" />
      <Label fx:id="reminderLabel" layoutX="57.0" layoutY="24.0" prefHeight="20.0" prefWidth="735.0">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Button fx:id="appointmentButton" layoutX="656.0" layoutY="52.0" mnemonicParsing="false" onAction="#toAppointmentPage" prefHeight="25.0" prefWidth="140.0" text="Appointments" />
      <Button fx:id="reportsButton" layoutX="506.0" layoutY="52.0" mnemonicParsing="false" onAction="#toReportPage" prefHeight="25.0" prefWidth="140.0" text="Reports" />
   </children>
</AnchorPane>
